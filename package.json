{
  "name": "ipsql",
  "version": "0.0.0-dev",
  "description": "Decentralized SQL Database",
  "main": "index.js",
  "type": "module",
  "bin": {
    "ipsql": "./cli/bin.js"
  },
  "scripts": {
    "build": "npm_config_yes=true npx ipjs@latest build --tests",
    "publish": "npm_config_yes=true npx ipjs@latest publish",
    "lint": "standard",
    "test:cjs": "npm run build && mocha dist/cjs/node-test/test-*.js && npm run test:cjs:browser",
    "test:node": "mocha test/test-*.js",
    "test:cjs:browser": "polendina --cleanup dist/cjs/browser-test/test-*.js",
    "test": "standard && npm run test:node",
    "test:node-v12": "mocha test/test-*.js && npm run test:cjs",
    "test:node-v14": "npm run test:node-v12",
    "coverage": "c8 --reporter=html mocha test/test-*.js && npm_config_yes=true npx st -d coverage -p 8080"
  },
  "keywords": [],
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com> (https://www.mikealrogers.com/)",
  "license": "(Apache-2.0 AND MIT)",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./cli": {
      "import": "./cli/cli.js"
    },
    "./utils": {
      "import": "./src/utils.js"
    },
    "./cache": {
      "import": "./src/cache.js"
    },
    "./network": {
      "import": "./src/network.js"
    },
    "./csv": {
      "import": "./src/csv.js"
    }
  },
  "devDependencies": {
    "hundreds": "0.0.9",
    "mocha": "^8.2.1",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^2.0.3",
    "@textile/ipfs-lite": "^0.1.6",
    "chunky-trees": "^0.3.2",
    "cliffy": "^2.4.2",
    "dag-service": "0.0.0-dev",
    "get-port": "^5.1.1",
    "interface-datastore": "^2.0.1",
    "ipfs": "^0.52.3",
    "ipfs-repo": "^7.0.0",
    "ipfsd-ctl": "^7.2.0",
    "libp2p": "^0.30.0",
    "libp2p-kad-dht": "^0.20.4",
    "libp2p-mplex": "^0.10.1",
    "libp2p-noise": "^2.0.1",
    "libp2p-tcp": "^0.15.1",
    "multiformats": "^4.4.1",
    "papaparse": "^5.3.0",
    "public-ip": "^4.0.3",
    "tempy": "^1.0.0",
    "yargs": "^16.2.0",
    "znode": "^1.1.5"
  }
}
